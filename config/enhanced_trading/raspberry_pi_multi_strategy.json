{
  "system": {
    "name": "Raspberry Pi Multi-Strategy Trading Service",
    "version": "2.0.0",
    "description": "Single service running multiple strategy instances with paper/live broker configuration",
    "environment": "production",
    "max_concurrent_strategies": 10,
    "health_check_interval": 60,
    "auto_recovery": true,
    "auto_recovery_max_attempts": 3,
    "monitoring_enabled": true,
    "log_level": "INFO"
  },
  
  "strategies": [
    {
      "id": "rsi_atr_btc_paper",
      "name": "RSI+ATR BTC Paper Trading",
      "enabled": true,
      "symbol": "BTCUSDT",
      "broker": {
        "type": "binance",
        "trading_mode": "paper",
        "name": "rsi_atr_btc_paper_broker",
        "cash": 5000.0,
        "paper_trading_config": {
          "mode": "realistic",
          "initial_balance": 5000.0,
          "commission_rate": 0.001,
          "slippage_model": "linear",
          "base_slippage": 0.0005,
          "latency_simulation": true,
          "min_latency_ms": 20,
          "max_latency_ms": 100,
          "market_impact_enabled": true,
          "realistic_fills": true,
          "partial_fill_probability": 0.1,
          "reject_probability": 0.02
        }
      },
      "strategy": {
        "type": "CustomStrategy",
        "parameters": {
          "entry_logic": {
            "name": "RSIBBVolumeEntryMixin",
            "params": {
              "e_rsi_period": 14,
              "e_rsi_oversold": 30,
              "e_bb_period": 20,
              "e_bb_dev": 2.0,
              "e_vol_ma_period": 20,
              "e_min_volume_ratio": 1.1,
              "e_use_bb_touch": true
            }
          },
          "exit_logic": {
            "name": "ATRExitMixin",
            "params": {
              "x_atr_period": 14,
              "x_sl_multiplier": 1.5
            }
          },
          "position_size": 0.1
        }
      },
      "data": {
        "data_source": "binance",
        "interval": "1h",
        "lookback_bars": 500
      },
      "trading": {
        "position_size": 0.1,
        "max_positions": 1
      },
      "risk_management": {
        "max_position_size": 1000.0,
        "stop_loss_pct": 3.0,
        "take_profit_pct": 6.0,
        "max_daily_loss": 200.0,
        "max_daily_trades": 5
      },
      "notifications": {
        "position_opened": true,
        "position_closed": true,
        "email_enabled": false,
        "telegram_enabled": true,
        "error_notifications": true
      }
    },
    
    {
      "id": "rsi_atr_btc_live",
      "name": "RSI+ATR BTC Live Trading",
      "enabled": false,
      "_comment": "LIVE TRADING - Enable only when ready for real money",
      "symbol": "BTCUSDT",
      "broker": {
        "type": "binance",
        "trading_mode": "live",
        "name": "rsi_atr_btc_live_broker",
        "cash": 1000.0,
        "live_trading_confirmed": false,
        "_warning": "LIVE TRADING MODE - REAL MONEY WILL BE USED"
      },
      "strategy": {
        "type": "CustomStrategy",
        "parameters": {
          "entry_logic": {
            "name": "RSIBBVolumeEntryMixin",
            "params": {
              "e_rsi_period": 14,
              "e_rsi_oversold": 25,
              "e_bb_period": 20,
              "e_bb_dev": 2.0,
              "e_vol_ma_period": 20,
              "e_min_volume_ratio": 1.2,
              "e_use_bb_touch": true
            }
          },
          "exit_logic": {
            "name": "ATRExitMixin",
            "params": {
              "x_atr_period": 14,
              "x_sl_multiplier": 2.0
            }
          },
          "position_size": 0.05
        }
      },
      "data": {
        "data_source": "binance",
        "interval": "1h",
        "lookback_bars": 500
      },
      "trading": {
        "position_size": 0.05,
        "max_positions": 1
      },
      "risk_management": {
        "max_position_size": 200.0,
        "stop_loss_pct": 2.0,
        "take_profit_pct": 4.0,
        "max_daily_loss": 50.0,
        "max_daily_trades": 3,
        "trailing_stop_enabled": true,
        "trailing_stop_percentage": 1.0
      },
      "notifications": {
        "position_opened": true,
        "position_closed": true,
        "email_enabled": true,
        "telegram_enabled": true,
        "error_notifications": true
      }
    },
    
    {
      "id": "bb_rsi_eth_paper",
      "name": "BB+RSI ETH Paper Trading",
      "enabled": true,
      "symbol": "ETHUSDT",
      "broker": {
        "type": "binance",
        "trading_mode": "paper",
        "name": "bb_rsi_eth_paper_broker",
        "cash": 3000.0,
        "paper_trading_config": {
          "mode": "realistic",
          "initial_balance": 3000.0,
          "commission_rate": 0.001,
          "slippage_model": "linear",
          "base_slippage": 0.0006,
          "latency_simulation": true,
          "min_latency_ms": 25,
          "max_latency_ms": 120,
          "market_impact_enabled": true,
          "realistic_fills": true,
          "partial_fill_probability": 0.12,
          "reject_probability": 0.015
        }
      },
      "strategy": {
        "type": "CustomStrategy",
        "parameters": {
          "entry_logic": {
            "name": "RSIOrBBEntryMixin",
            "params": {
              "e_rsi_period": 14,
              "e_rsi_oversold": 30,
              "e_bb_period": 20,
              "e_bb_dev": 2.0,
              "e_cooldown_bars": 3
            }
          },
          "exit_logic": {
            "name": "RSIBBExitMixin",
            "params": {
              "x_rsi_period": 14,
              "x_rsi_overbought": 70,
              "x_bb_period": 20,
              "x_bb_dev": 2.0,
              "x_use_bb_touch": true
            }
          },
          "position_size": 0.15
        }
      },
      "data": {
        "data_source": "binance",
        "interval": "2h",
        "lookback_bars": 400
      },
      "trading": {
        "position_size": 0.15,
        "max_positions": 1
      },
      "risk_management": {
        "max_position_size": 800.0,
        "stop_loss_pct": 2.5,
        "take_profit_pct": 5.0,
        "max_daily_loss": 150.0,
        "max_daily_trades": 4
      },
      "notifications": {
        "position_opened": true,
        "position_closed": true,
        "email_enabled": false,
        "telegram_enabled": true,
        "error_notifications": true
      }
    },
    
    {
      "id": "macd_atr_ada_paper",
      "name": "MACD+ATR ADA Paper Trading",
      "enabled": true,
      "symbol": "ADAUSDT",
      "broker": {
        "type": "binance",
        "trading_mode": "paper",
        "name": "macd_atr_ada_paper_broker",
        "cash": 2000.0,
        "paper_trading_config": {
          "mode": "realistic",
          "initial_balance": 2000.0,
          "commission_rate": 0.001,
          "slippage_model": "sqrt",
          "base_slippage": 0.0008,
          "latency_simulation": true,
          "min_latency_ms": 30,
          "max_latency_ms": 150,
          "market_impact_enabled": true,
          "realistic_fills": true,
          "partial_fill_probability": 0.15,
          "reject_probability": 0.02
        }
      },
      "strategy": {
        "type": "CustomStrategy",
        "parameters": {
          "entry_logic": {
            "name": "MACDEntryMixin",
            "params": {
              "e_macd_fast": 12,
              "e_macd_slow": 26,
              "e_macd_signal": 9,
              "e_min_histogram": 0.001
            }
          },
          "exit_logic": {
            "name": "ATRExitMixin",
            "params": {
              "x_atr_period": 14,
              "x_sl_multiplier": 1.8
            }
          },
          "position_size": 0.2
        }
      },
      "data": {
        "data_source": "binance",
        "interval": "4h",
        "lookback_bars": 300
      },
      "trading": {
        "position_size": 0.2,
        "max_positions": 1
      },
      "risk_management": {
        "max_position_size": 500.0,
        "stop_loss_pct": 4.0,
        "take_profit_pct": 8.0,
        "max_daily_loss": 100.0,
        "max_daily_trades": 3
      },
      "notifications": {
        "position_opened": true,
        "position_closed": true,
        "email_enabled": false,
        "telegram_enabled": true,
        "error_notifications": true
      }
    }
  ],
  
  "global_settings": {
    "notifications": {
      "system_notifications": true,
      "strategy_notifications": true,
      "email_enabled": false,
      "telegram_enabled": true,
      "notification_levels": ["ERROR", "WARNING", "TRADE", "INFO"]
    },
    "monitoring": {
      "performance_tracking": true,
      "health_monitoring": true,
      "resource_monitoring": true,
      "alert_thresholds": {
        "cpu_usage": 80,
        "memory_usage": 85,
        "temperature": 70,
        "error_rate": 5
      }
    },
    "logging": {
      "level": "INFO",
      "file": "logs/enhanced_trading/multi_strategy_service.log",
      "max_size_mb": 100,
      "backup_count": 5,
      "console_output": true
    },
    "database": {
      "url": "sqlite:///db/multi_strategy_trading.db",
      "backup_enabled": true,
      "backup_interval_hours": 24
    }
  }
}