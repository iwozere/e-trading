{
    "name": "EOMBreakoutEntryMixin",
    "timeframe": "1d",
    "trading_style": "position_trading",
    "description": "Breakout + EOM confirmation for daily position trades",
    "params": {
        "e_breakout_threshold": {
            "type": "float",
            "low": 0.005,
            "high": 0.015,
            "default": 0.008,
            "description": "Breakout threshold above resistance"
        },
        "e_use_atr_filter": {
            "type": "categorical",
            "choices": [true, false],
            "default": true,
            "description": "Use ATR filter to avoid low volatility periods"
        },
        "e_rsi_overbought": {
            "type": "int",
            "low": 70,
            "high": 80,
            "default": 75,
            "description": "RSI overbought threshold"
        }
    },
    "indicator_params": {
        "eom_timeperiod": {
            "type": "int",
            "low": 12,
            "high": 22,
            "default": 14,
            "description": "EOM smoothing period"
        },
        "sr_lookback_bars": {
            "type": "int",
            "low": 3,
            "high": 7,
            "default": 4,
            "description": "S/R swing detection lookback"
        },
        "volume_sma_period": {
            "type": "int",
            "low": 18,
            "high": 30,
            "default": 20,
            "description": "Volume SMA period"
        },
        "atr_period": {
            "type": "int",
            "low": 12,
            "high": 22,
            "default": 14,
            "description": "ATR period"
        },
        "atr_sma_period": {
            "type": "int",
            "low": 80,
            "high": 120,
            "default": 100,
            "description": "ATR SMA period for trend filter"
        },
        "rsi_period": {
            "type": "int",
            "low": 12,
            "high": 22,
            "default": 14,
            "description": "RSI period"
        }
    }
}
