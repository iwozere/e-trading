{
    "name": "EOMPullbackEntryMixin",
    "timeframe": "5m",
    "trading_style": "day_trading",
    "description": "Pullback to support + EOM reversal for 5m scalping",
    "params": {
        "e_support_threshold": {
            "type": "float",
            "low": 0.003,
            "high": 0.008,
            "default": 0.005,
            "description": "Support bounce threshold (0.3%-0.8%)"
        },
        "e_rsi_oversold": {
            "type": "int",
            "low": 25,
            "high": 45,
            "default": 35,
            "description": "RSI oversold threshold (more aggressive for 5m)"
