        },
        "sr_lookback_bars": {
            "type": "int",
            "low": 2,
            "high": 5,
            "default": 3,
            "description": "S/R swing detection lookback"
        },
        "rsi_period": {
            "type": "int",
            "low": 12,
            "high": 20,
            "default": 14,
            "description": "RSI period"
